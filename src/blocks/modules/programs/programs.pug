include ../.././components/card/card-data.pug
include ../.././components/card/card-program.pug

section.programs
    .l-default
        h2.font-h2 Пакет программ TEPLOOV
        .programs__items
            each program_card in program_cards
                +card-program({
                    href: program_card.href,
                    title: program_card.title,
                    price: program_card.price,
                    text: program_card.text,
                    date: program_card.date,
                })